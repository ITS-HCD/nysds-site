    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>{% if section != 'home' %}{% if title %}{{ title | safe }} - {% endif %}{% endif %}{{ site.title }}</title>
    {% if description %}<meta name="description" content="{{ description }}">{% endif %}   
    <link rel="canonical" href="{{ site.url }}{{page.url}}">

    <meta name=”robots” content=”noindex”>

    <meta property="og:title" content="{{ title | safe }}">
    <meta property="og:site_name" content="{{ site.title }}">
    <meta property="og:url" content="{{ site.url }}{{page.url}}">
    <meta property="og:description" content="{{ description }}">
    <meta property="og:type" content="{%- if ogtype -%}{{ ogtype }}{%- else -%}website{%- endif -%}">
    <meta property="og:locale" content="en_US">
    
    {# TODO: Add custom placeholder image
    <meta property="og:image" content="{{ site.url }}/assets/img/logo.png">
    <meta property="og:image:width" content="1200">
    <meta property="og:image:height" content="630">
    #}
    <link rel="preload" href="{{ site.url }}/assets/fonts/proximanova-regular.woff2" as="font" type="font/woff2" crossorigin>
    <link rel="stylesheet" href="/assets/fonts/excelsior-fonts.css">    
    <link rel="stylesheet" href="{{ site.url }}/assets/css/vendor/excelsior-full.min.css">    
    <link rel="stylesheet" href="{{ site.url }}/assets/css/excelsior-site.css">    
    <link href="{{ site.url }}/pagefind/pagefind-ui.css" rel="stylesheet">
    <link href="https://unpkg.com/prismjs@1.29.0/themes/prism-coy.css" rel="stylesheet">
