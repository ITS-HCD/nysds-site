<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">

<title>{% if section != 'home' %}{% if title %}{{ title | safe }} - {% endif %}{% endif %}{{ site.title }}</title>
{% if description %}
<meta name="description" content="{{ description }}">{% endif %}
<link rel="canonical" href="{{ site.url }}{{page.url}}">
<link rel="icon" type="image/svg+xml" href="{{ site.url }}/favicon.svg" />
<link rel="apple-touch-icon" sizes="180x180" href="{{ site.url }}/apple-touch-icon.png">


<meta property="og:title" content="{{ title | safe }}">
<meta property="og:site_name" content="{{ site.title }}">
<meta property="og:url" content="{{ site.url }}{{page.url}}">
<meta property="og:description" content="{{ description }}">
<meta property="og:type" content="{%- if ogtype -%}{{ ogtype }}{%- else -%}website{%- endif -%}">
<meta property="og:locale" content="en_US">
{% if not ogimage %}
<meta property="og:image" content="{{ site.url }}/assets/img/nysds_og_image.png">
{% else %}
<meta property="og:image" content="{{ site.url }}{{ ogimage }}">
{% endif %}
<meta property="og:image:width" content="1200">
<meta property="og:image:height" content="630">
<meta property="og:logo" content="{{ site.url }}/assets/img/nysds-logo-mark.png">
<meta name="google-site-verification" content="3HnIZEb3eGAR0wp23L9DnHO-pVHPXkewO5BuVaXlrSk">

<link href="{{ site.url | url }}/assets/css/vendor/nysds-full.min.css" rel="stylesheet">
<link rel="preload" href="{{ site.url | url }}/assets/fonts/proximanova-regular.woff" as="font" type="font/woff"
  crossorigin>
<link href="{{ site.url | url}}/assets/fonts/nysds-fonts.css" rel="stylesheet">
<link href="{{ site.url | url}}/pagefind/pagefind-ui.css" rel="stylesheet">
<link href="{{ site.url | url }}/assets/css/prism-theme.css" rel="stylesheet">
<link href="{{ site.url | url }}/assets/css/nysds-site.css" rel="stylesheet">

<!-- Google tag (gtag.js) -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-M4JYBJ7BZY"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag() { dataLayer.push(arguments); }
  gtag('js', new Date());

  gtag('config', 'G-M4JYBJ7BZY');
</script>